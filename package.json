{
  "name": "data-lackey",
  "version": "1.0.0-alpha.2",
  "description": "orchestrates loading of your data",
  "main": "dist/index.js",
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest --coverage",
    "lint": "./node_modules/.bin/eslint ./src/",
    "build": "babel src --ignore '*.spec.js' --out-dir dist",
    "prepublish": "yarn build"
  },
  "repository": "github:Verba/data-lackey.git",
  "keywords": [
    "fetch",
    "REST",
    "redux"
  ],
  "author": {
    "name": "Andrew J. Peterson",
    "email": "andy@ndpsoftware.com",
    "url": "http://ndpsoftware.com/"
  },
  "license": "MIT",
  "engines": {},
  "bugs": {
    "url": "https://github.com/Verba/data-lackey/issues"
  },
  "homepage": "https://github.com/Verba/data-lackey#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "eslint": "*",
    "eslint-import-resolver-babel-module": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-no-for-of-loops": "*",
    "eslint-plugin-no-only-tests": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "hoist-non-react-statics": "^3.3.0",
    "jest": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "setupFiles": [
      "./src/jest/enzyme.js",
      "./src/jest/waitForAssertion.js"
    ],
    "setupFilesAfterEnv": [
      "./src/jest/defaultTimeout.js"
    ],
    "coveragePathIgnorePatterns": [
      "./src/jest"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 89,
        "functions": 94,
        "lines": 96
      }
    },
    "restoreMocks": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "url-pattern": "^1"
  }
}
